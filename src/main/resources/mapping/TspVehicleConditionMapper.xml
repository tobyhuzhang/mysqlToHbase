<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="qjm.data.synch.mapper.TspVehicleConditionMapper">
    <insert id="insertCondition" parameterType="qjm.data.synch.modle.TspVehicleCondition">
        insert into
        ims_tspvehiclecondition(AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp)
        value
        (#{acquisitionTime},#{batteryVoltage},#{engineSpeed},#{fuel},#{latitude},#{longitude},#{totalDistance},#{vehBatSOC},#{vehicleSpeed},#{vin},#{timestamp})
        <!--注意属性名区分大小写 -->
    </insert>

    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultType="qjm.data.synch.modle.TspVehicleCondition">
        select
            AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp
        from ims_tspvehiclecondition where id=#{id}
    </select>

    <select id="countAll" resultType="int">
        select count(1)
        from ims_tspvehiclecondition order by Id desc
    </select>

    <select id="selectThousand" parameterType="java.util.Map" resultType="qjm.data.synch.modle.TspVehicleCondition">
        select
        AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp
        from ims_tspvehiclecondition order by Id desc
    </select>

    <insert id="insertByBatch" parameterType="java.util.List">
        insert into ims_tspvehiclecondition
        (AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{acquisitionTime},#{batteryVoltage},#{engineSpeed},#{fuel},#{latitude},#{longitude},#{totalDistance},#{vehBatSOC},#{vehicleSpeed},#{vin},#{timestamp})
        </foreach>
    </insert>
</mapper>