<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="qjm.data.synch.mapper.TspConditionMapper">
    <insert id="insertCondition" parameterType="qjm.data.synch.modle.TspVehicleCondition">
        insert into
        ims_tspvehiclecondition(AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp)
        value
        (#{acquisitionTime},#{batteryVoltage},#{engineSpeed},#{fuel},#{latitude},#{longitude},#{totalDistance},#{vehBatSOC},#{vehicleSpeed},#{vin},#{timestamp})
        <!--注意属性名区分大小写 -->
    </insert>
    <insert id="insertEvent" parameterType="qjm.data.synch.modle.TspVehicleEvent">
        insert into
        ims_tspvehicleevent(Latitude,VIN,acquisitionTime,Event,TotalDistance,Timestamp,Longitude)
        value
        (#{latitude},#{vin},#{acquisitionTime},#{event},#{totalDistance},#{timestamp},#{longitude})
        <!--注意属性名区分大小写 -->
    </insert>
    <insert id="insertTempRecord" parameterType="qjm.data.synch.modle.TemperatureRecord">
        insert into
        ims_temp_record(VIN,deviceNumber,temperature1,timestamp,humidity,latitude,longitude,photoreceptor,electricity,maxStrength,devicetype)
        value
        (#{vin},#{deviceNumber},#{temperature1},#{timestamp},#{humidity},#{latitude},#{longitude},#{photoreceptor},#{electricity},#{maxStrength},#{deviceType})
        <!--注意属性名区分大小写 -->
    </insert>
    <insert id="insertCompleteCondition" parameterType="qjm.data.synch.modle.TspCompleteCondition">
        insert into
        ims_tsp_completecondition(AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp,temperature1,temperature2,temperature3,temperature4,temperature5,humidity1,humidity2,humidity3,humidity4,humidity5,photoreceptor1,photoreceptor2,photoreceptor3,photoreceptor4,photoreceptor5,temptimestamp1,temptimestamp2,temptimestamp3,temptimestamp4,temptimestamp5,plateLicenseNo)
        value
        (#{acquisitionTime},#{batteryVoltage},#{engineSpeed},#{fuel},#{latitude},#{longitude},#{totalDistance},#{vehBatSOC},#{vehicleSpeed},#{vin},#{timestamp},#{temperature1},#{temperature2},#{temperature3},#{temperature4},#{temperature5},#{humidity1},#{humidity2},#{humidity3},#{humidity4},#{humidity5},#{photoreceptor1},#{photoreceptor2},#{photoreceptor3},#{photoreceptor4},#{photoreceptor5},#{temptimestamp1},#{temptimestamp2},#{temptimestamp3},#{temptimestamp4},#{temptimestamp5},#{plateLicenseNo})
        <!--注意属性名区分大小写 -->
    </insert>
    <insert id="insertAoyiCondition" parameterType="qjm.data.synch.modle.TspAoyiCondition">
        insert into
        ims_aoyi_condition
        (deviceNumber,acquisitionTime,latitude,longitude,totalDistance,batSOC,vehicleSpeed,vin,plateLicenseNo,runningTime,parkingTime,relativeMileage,sumMileage,temperature,status,timestamp)
        value
        (#{deviceNumber},#{acquisitionTime},#{latitude},#{longitude},#{totalDistance},#{batSOC},#{vehicleSpeed},#{vin},#{plateLicenseNo},#{runningTime},#{parkingTime},#{relativeMileage},#{sumMileage},#{temperature},#{status},#{timestamp})
        <!--注意属性名区分大小写 -->
    </insert>

    <select id="selectThousand" parameterType="java.util.Map" resultType="qjm.data.synch.modle.TspCompleteCondition">
        select
        AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp,temperature1,temperature2,temperature3,temperature4,temperature5,humidity1,humidity2,humidity3,humidity4,humidity5,photoreceptor1,photoreceptor2,photoreceptor3,photoreceptor4,photoreceptor5,temptimestamp1,temptimestamp2,temptimestamp3,temptimestamp4,temptimestamp5,plateLicenseNo
        <!--<include refid="Base_Column_List" />-->
        from ims_tsp_completecondition order by Id desc
        <if test="offset != null and limit != null">
            limit #{offset}, #{limit}
        </if>
    </select>

    <select id="countAll" resultType="int">
        select count(1)
        <!--<include refid="Base_Column_List" />-->
        from ims_tsp_completecondition order by Id desc
    </select>

    <insert id="insertByBatch" parameterType="java.util.List">
        insert into ims_tsp_completecondition
        (AcquisitionTime,batteryVoltage,EngineSpeed,Fuel,Latitude,Longitude,TotalDistance,VehBatSOC,VehicleSpeed,VIN,Timestamp,temperature1,temperature2,temperature3,temperature4,temperature5,humidity1,humidity2,humidity3,humidity4,humidity5,photoreceptor1,photoreceptor2,photoreceptor3,photoreceptor4,photoreceptor5,temptimestamp1,temptimestamp2,temptimestamp3,temptimestamp4,temptimestamp5,plateLicenseNo)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{acquisitionTime},#{batteryVoltage},#{engineSpeed},#{fuel},#{latitude},#{longitude},#{totalDistance},#{vehBatSOC},#{vehicleSpeed},#{vin},#{timestamp},#{temperature1},#{temperature2},#{temperature3},#{temperature4},#{temperature5},#{humidity1},#{humidity2},#{humidity3},#{humidity4},#{humidity5},#{photoreceptor1},#{photoreceptor2},#{photoreceptor3},#{photoreceptor4},#{photoreceptor5},#{temptimestamp1},#{temptimestamp2},#{temptimestamp3},#{temptimestamp4},#{temptimestamp5},#{plateLicenseNo})
        </foreach>
    </insert>
</mapper>